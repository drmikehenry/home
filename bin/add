#!/usr/bin/env python3

import fileinput
import re


def get_num(line: str) -> float:
    m = re.search(
        r"""
            [-+]?
            (
                \d+\.?\d*
            |
                \.\d+
            )
        """,
        line,
        re.VERBOSE,
    )
    if m:
        num = float(m.group(0))
    else:
        num = 0.0
    return num


def main() -> None:
    total = 0.0
    for line in fileinput.input():
        total += get_num(line)
    total_str = str(total).rstrip(".0")
    print(total_str)


if __name__ == "__main__":
    main()
